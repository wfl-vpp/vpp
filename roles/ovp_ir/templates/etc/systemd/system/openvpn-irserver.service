[Unit]
Description=OpenVPN Service - Iran server service

[Service]
Type=forking
User=root
WorkingDirectory={{ ovpn_root }}
ExecStart=/usr/sbin/openvpn --config {{ ovpn_root }}/ovpn-irserver.conf --writepid /run/openvpn_irserver/PID
Restart=always
PIDFile=/run/openvpn_irserver/PID

[Install]
WantedBy=multi-user.target
